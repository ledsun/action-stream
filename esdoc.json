{
  "source": ".",
  "destination": "./esdoc",
  "includes": ["\\.mjs$"],
  "excludes": ["(node_modules|esdocs|test|babel-plugin-transform-import-extension)"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./test/",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(spec|Spec|test|Test)\\.mjs$"],
          "excludes": ["\\.config\\.js$"]
        }
      }
    }
  ]
}
